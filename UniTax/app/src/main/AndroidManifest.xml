<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tommik.unitax">

    <!--         Se vogliamo forzare il portrait:  android:screenOrientation="portrait" -->
    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/UniTaxTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TutorialActivity" />
        <activity android:name=".SelectUniActivity" />
        <activity android:name=".InfoUniActivity" />
        <activity android:name=".DataTaxActivity" />
        <activity android:name=".ResultTaxActivity" />
        <activity android:name=".CalcoloTaxActivity"></activity>
    </application>

</manifest>